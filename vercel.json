{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "hooks": {
    "postinstall": "pip install -r requirements.txt",
    "build": "python manage.py collectstatic"
  }
}
